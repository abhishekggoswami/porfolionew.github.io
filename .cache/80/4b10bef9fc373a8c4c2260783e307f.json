ow-blue1: rgba(31, 38, 135, 0.27);\r\n$box-shadow-blue2: rgba(31, 38, 135, 0.67);\r\n\r\n\r\n/*---------prdefined section-----------*/\r\n.section{\r\n    padding: 1rem;\r\n    overflow: hidden;\r\n    min-height: 100vh;\r\n    padding-top: 5rem;\r\n    @media (min-width: 650px){\r\n        padding : 5rem;\r\n    }\r\n    &__header{\r\n        &-title{\r\n            font-weight: 400;\r\n            font-size: 17px;\r\n            color: #666;\r\n\r\n        }\r\n        &-subtitle{\r\n            font-size: 27px;\r\n            font-weight: 700;\r\n        }\r\n    }\r\n\r\n\r\n}\r\n\r\n\r\n//button \r\n.containerr {\r\n\twidth: 1000px;\r\n\tdisplay: flex;\r\n\tflex-wrap: wrap;\r\n\tjustify-content: space-around;\r\n    z-index:5;\r\n    position:absolute;\r\n    left: -22%;\r\n    bottom: 17%;\r\n}\r\n.containerr .btn {\r\n\tposition: relative;\r\n\ttop: 0;\r\n\tleft: 0;\r\n\twidth: 250px;\r\n\theight: 50px;\r\n\tmargin: 0;\r\n\tdisplay: flex;\r\n\tjustify-content: center;\r\n\talign-items: center;\r\n}\r\n.containerr .btn a {\r\n\tposition: absolute;\r\n\ttop: 0;\r\n\tleft: 0;\r\n\twidth: 100%;\r\n\theight: 100%;\r\n\tdisplay: flex;\r\n\tjustify-content: center;\r\n\talign-items: center;\r\n\tbackground: rgba(255, 255, 255, 0.05);\r\n\tbox-shadow: 0 15px 15px rgba(0, 0, 0, 0.3);\r\n\tborder-bottom: 1px solid rgba(255, 255, 255, 0.1);\r\n\tborder-top: 1px solid rgba(255, 255, 255, 0.1);\r\n\tborder-radius: 30px;\r\n\tpadding: 10px;\r\n\tletter-spacing: 1px;\r\n\ttext-decoration: none;\r\n\toverflow: hidden;\r\n\tcolor: #fff;\r\n\tfont-weight: 400px;\r\n\tz-index: 1;\r\n\ttransition: 0.5s;\r\n\tbackdrop-filter: blur(15px);\r\n}\r\n.containerr .btn:hover a {\r\n\tletter-spacing: 3px;\r\n}\r\n.containerr .btn a::before {\r\n\tcontent: \"\";\r\n\tposition: absolute;\r\n\ttop: 0;\r\n\tleft: 0;\r\n\twidth: 50%;\r\n\theight: 100%;\r\n\tbackground: linear-gradient(to left, rgba(255, 255, 255, 0.15), transparent);\r\n\ttransform: skewX(45deg) translate(0);\r\n\ttransition: 0.5s;\r\n\tfilter: blur(0px);\r\n}\r\n.containerr .btn:hover a::before {\r\n\ttransform: skewX(45deg) translate(200px);\r\n}\r\n.containerr .btn::before {\r\n\tcontent: \"\";\r\n\tposition: absolute;\r\n\tleft: 50%;\r\n\ttransform: translatex(-50%);\r\n\tbottom: -5px;\r\n\twidth: 30px;\r\n\theight: 10px;\r\n\tbackground: #f00;\r\n\tborder-radius: 10px;\r\n\ttransition: 0.5s;\r\n\ttransition-delay: 0.5;\r\n}\r\n.containerr .btn:hover::before /*lightup button*/ {\r\n\tbottom: 0;\r\n\theight: 50%;\r\n\twidth: 80%;\r\n\tborder-radius: 30px;\r\n}\r\n\r\n.containerr .btn::after {\r\n\tcontent: \"\";\r\n\tposition: absolute;\r\n\tleft: 50%;\r\n\ttransform: translatex(-50%);\r\n\ttop: -5px;\r\n\twidth: 30px;\r\n\theight: 10px;\r\n\tbackground: #f00;\r\n\tborder-radius: 10px;\r\n\ttransition: 0.5s;\r\n\ttransition-delay: 0.5;\r\n}\r\n.containerr .btn:hover::after /*lightup button*/ {\r\n\ttop: 0;\r\n\theight: 50%;\r\n\twidth: 80%;\r\n\tborder-radius: 30px;\r\n}\r\n.containerr .btn:nth-child(1)::before, /*chnage 1*/\r\n.containerr .btn:nth-child(1)::after {\r\n\tbackground: 0xff0000;\r\n\tbox-shadow: 0 0 5px 0xff0000, 0 0 15px 0xff0000, 0 0 30px 0xff0000,\r\n\t\t0 0 60px 0xff0000;\r\n}\r\n\r\n\r\n/* For tablets (screens between 600px and 768px wide) */\r\n@media (max-width: 768px) and (min-width: 600px) {\r\n    .containerr {\r\n      width: 90%;\r\n      \r\n    }\r\n    .containerr .btn {\r\n      width: 200px;\r\n      height: 45px;\r\n    }\r\n    .containerr .btn a {\r\n      font-size: 14px;\r\n    }\r\n  }\r\n\r\n  @media (max-width: 752px) {\r\n    .containerr {\r\n      width: 90%;\r\n      \r\n    }\r\n    .containerr .btn {\r\n      width: 150px;\r\n      height: 40px;\r\n    }\r\n    .containerr .btn a {\r\n      font-size: 12px;\r\n    }\r\n  }\r\n  \r\n  /* For mobile devices (screens less than 600px wide) */\r\n  @media (max-width: 600px) {\r\n    .containerr {\r\n      width: 90%;\r\n    }\r\n    .containerr .btn {\r\n      width: 150px;\r\n      height: 40px